<?xml version="1.0" encoding="utf-8"?>

<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#DDF7F7"
    app:layout_constraintVertical_weight="1"
    tools:context=".Page12"
    android:layout_weight="3"
    android:orientation="vertical">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="-20sp"
        app:layout_constraintVertical_weight="1"
        tools:ignore="MissingConstraints">
    <ImageView
        android:id="@+id/TopImage"
        android:layout_width="250dp"
        android:layout_height="242dp"
        app:srcCompat="@drawable/e2d3b048a6d38fa3e27cc9fc7745fa3a"
        tools:ignore="MissingConstraints"
        android:contentDescription="@string/todo" />
    <ImageView
        android:id="@+id/arrowtop"
        android:layout_width="30dp"
        android:layout_height="40dp"
        android:layout_marginTop="50dp"
        android:layout_marginStart="15dp"
        android:src="@drawable/baseline_arrow_back_24"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:contentDescription="@string/todo"/>
    <TextView
        android:id="@+id/bookedsession"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="@string/add_new_mentor"
        android:textColor="@color/darkgreen"
        android:textSize="32sp"
        android:textStyle="bold"
        app:layout_constraintTop_toBottomOf="@id/arrowtop"
        app:layout_constraintStart_toEndOf="@id/arrowtop"

        />
    <TextView
        android:id="@+id/name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="2dp"
        android:text="@string/help_us_expand"
        android:textStyle="bold"
        android:textColor="@color/darkgreen"
        android:textSize="15sp"
        app:layout_constraintTop_toBottomOf="@id/bookedsession"
        app:layout_constraintEnd_toEndOf="@id/bookedsession"
        />
    </androidx.constraintlayout.widget.ConstraintLayout>

<androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#50C0C7"
        android:layout_marginTop="-15sp"
        app:layout_constraintVertical_weight="2"
        tools:ignore="MissingConstraints">
    <TextView
        android:id="@+id/mid"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="@+id/background"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        />
    <ImageView
        android:id="@+id/background"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:srcCompat="@drawable/darkgreenrectanglechat"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:contentDescription="@string/todo"
        />
    <ImageView
        android:id="@+id/rec1"
        android:layout_width="110dp"
        android:layout_height="110dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="30dp"
        android:src="@drawable/whiterectanglr"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="@id/mid"
        app:layout_constraintTop_toTopOf="@id/background"
        android:contentDescription="@string/todo"
        />
    <ImageView
        android:id="@+id/videocamera"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="10dp"
        app:srcCompat="@drawable/baseline_missed_video_call_24"
        app:layout_constraintStart_toStartOf="@id/rec1"
        app:layout_constraintEnd_toEndOf="@id/rec1"
        app:layout_constraintTop_toTopOf="@id/rec1"
        app:layout_constraintBottom_toBottomOf="@id/rec1"
        android:contentDescription="@string/todo"
        />
    <TextView
        android:id="@+id/videotext"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textStyle="bold"
        android:layout_marginTop="-5dp"
        android:text="@string/upload_video"
        android:textColor="@color/darkgreen"
        android:textSize="15sp"
        app:layout_constraintTop_toBottomOf="@id/videocamera"
        app:layout_constraintStart_toStartOf="@id/videocamera"
        app:layout_constraintEnd_toEndOf="@id/videocamera"
        />

    <ImageView
        android:id="@+id/rec2"
        android:layout_width="110dp"
        android:layout_height="110dp"
        android:layout_marginEnd="10dp"
        android:layout_marginTop="30dp"
        android:src="@drawable/whiterectanglr"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@id/mid"
        app:layout_constraintTop_toTopOf="@id/background"
        android:contentDescription="@string/todo"
        />
    <ImageView
        android:id="@+id/camera"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="10dp"
        app:srcCompat="@drawable/outline_camera_alt_24"
        app:layout_constraintStart_toStartOf="@id/rec2"
        app:layout_constraintEnd_toEndOf="@id/rec2"
        app:layout_constraintTop_toTopOf="@id/rec2"
        app:layout_constraintBottom_toBottomOf="@id/rec2"
        android:contentDescription="@string/todo"
        />
    <TextView
        android:id="@+id/cameratext"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textStyle="bold"
        android:layout_marginTop="-5dp"
        android:text="@string/upload_photo"
        android:textColor="@color/darkgreen"
        android:textSize="15sp"
        app:layout_constraintTop_toBottomOf="@id/camera"
        app:layout_constraintStart_toStartOf="@id/camera"
        app:layout_constraintEnd_toEndOf="@id/camera"
        />
    <ImageView
        android:id="@+id/PostedPhoto"
        android:layout_width="110dp"
        android:layout_height="110dp"
        android:layout_marginEnd="10dp"
        android:layout_marginTop="30dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@id/mid"
        app:layout_constraintTop_toTopOf="@id/background"
        android:contentDescription="@string/todo"
        />
    <TextView
        android:id="@+id/Name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="260dp"
        android:layout_marginTop="20dp"
        android:text="@string/name"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintTop_toBottomOf="@+id/rec1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        />

    <ImageView
        android:id="@+id/emailinputback"
        android:layout_width="match_parent"
        android:layout_height="48dp"
        android:paddingLeft="50dp"
        android:paddingRight="50dp"
        android:src="@drawable/whiterectanglr"
        app:layout_constraintTop_toBottomOf="@+id/Name"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:contentDescription="@string/todo"
        />
    <EditText
        android:id="@+id/Nameinput"
        android:layout_width="280dp"
        android:layout_height="46dp"
        android:ems="10"
        android:hint="@string/enter_name"
        android:inputType="textEmailAddress"
        app:layout_constraintTop_toBottomOf="@+id/Name"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:autofillHints="" />
    <TextView
        android:id="@+id/Des"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="220dp"
        android:layout_marginTop="15dp"
        android:text="@string/description"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintTop_toBottomOf="@+id/Nameinput"
        app:layout_constraintStart_toStartOf="@+id/Nameinput"
        />
    <ImageView
        android:id="@+id/descinputback"
        android:layout_width="match_parent"
        android:layout_height="48dp"
        android:paddingLeft="50dp"
        android:paddingRight="50dp"
        android:src="@drawable/whiterectanglr"
        app:layout_constraintTop_toBottomOf="@+id/Des"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:contentDescription="@string/todo"
        />
    <EditText
        android:id="@+id/Desinput"
        android:layout_width="280dp"
        android:layout_height="46dp"
        android:ems="10"
        android:hint="@string/description"
        android:inputType="textEmailAddress"
        app:layout_constraintTop_toBottomOf="@+id/Des"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:autofillHints="" />
    <TextView
        android:id="@+id/Status"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="220dp"
        android:layout_marginTop="15dp"
        android:text="@string/status"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintTop_toBottomOf="@+id/Desinput"
        app:layout_constraintStart_toStartOf="@+id/Desinput"
        />
    <ImageView
        android:id="@+id/statusinputback"
        android:layout_width="match_parent"
        android:layout_height="48dp"
        android:paddingLeft="50dp"
        android:paddingRight="50dp"
        android:src="@drawable/whiterectanglr"
        app:layout_constraintTop_toBottomOf="@+id/Status"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:contentDescription="@string/todo"
        />
    <EditText
        android:id="@+id/statusinput"
        android:layout_width="280dp"
        android:layout_height="46dp"
        android:ems="10"
        android:hint="@string/available"
        android:inputType="textEmailAddress"
        app:layout_constraintTop_toBottomOf="@+id/Status"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        tools:ignore="ContentDescription"
        android:autofillHints="" />

    <ImageView
        android:id="@+id/Bottomrec"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        android:layout_marginTop="30sp"
        app:srcCompat="@drawable/darkestgreenrectangle"
        app:layout_constraintTop_toBottomOf="@+id/statusinputback"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:contentDescription="@string/todo"
        />

    <TextView
        android:id="@+id/bottomrectext"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textStyle="bold"
        android:layout_marginBottom="10dp"
        android:text="@string/upload"
        android:textColor="@color/white"
        android:textSize="23sp"
        app:layout_constraintBottom_toBottomOf="@id/Bottomrec"
        app:layout_constraintStart_toStartOf="@id/Bottomrec"
        app:layout_constraintTop_toTopOf="@+id/Bottomrec"
        app:layout_constraintEnd_toEndOf="@id/Bottomrec"
        />

    </androidx.constraintlayout.widget.ConstraintLayout>
</LinearLayout>