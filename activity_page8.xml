<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/lightgreen"
    tools:context=".Page8">

    <ImageView
        android:id="@+id/TopImage"
        android:layout_width="250dp"
        android:layout_height="242dp"
        app:srcCompat="@drawable/e2d3b048a6d38fa3e27cc9fc7745fa3a"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:contentDescription="@string/todo" />

    <ImageView
        android:id="@+id/Toparrow"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="25dp"
        android:layout_marginTop="60dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/baseline_arrow_back_24"
        android:contentDescription="@string/todo"
        />


    <TextView
        android:id="@+id/LetsFind"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:text="@string/let_s_find"
        android:textSize="35sp"
        android:textStyle="bold"
        android:textColor="@color/darkgreen"
        app:layout_constraintStart_toStartOf="@id/Toparrow"
        app:layout_constraintTop_toTopOf="@id/Toparrow"
        />

    <ImageView
        android:id="@+id/search00"
        android:layout_marginTop="10dp"
        android:layout_width="360dp"
        android:layout_height="35dp"
        app:layout_constraintTop_toBottomOf="@id/LetsFind"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:srcCompat="@drawable/whiterecpage8"
        android:contentDescription="@string/todo" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="35dp"
        android:hint="@string/search"
        android:textSize="20sp"
        android:textColor="@color/darkgreen"
        app:layout_constraintStart_toStartOf="@id/search00"
        app:layout_constraintTop_toTopOf="@id/search00"
        app:layout_constraintBottom_toBottomOf="@id/search00"
        tools:ignore="TextFields"
        android:autofillHints="" />
    <TextView
        android:id="@id/search"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginBottom="7dp"
        android:text="@string/searchicon"
        android:textColor="@color/darkgreen"
        android:textSize="40sp"
        app:layout_constraintBottom_toBottomOf="@id/search00"
        app:layout_constraintStart_toStartOf="@id/search00"
        app:layout_constraintTop_toTopOf="@id/search00" />
    <TextView
        android:id="@+id/Recent"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/darkgreen"
        android:layout_marginStart="15dp"
        android:layout_marginTop="35dp"
        android:text="@string/recent_search"
        android:textSize="12sp"
        android:textStyle="bold"
        app:layout_constraintTop_toBottomOf="@+id/search00"
        app:layout_constraintStart_toStartOf="parent"
        />

    <TextView
        android:id="@+id/M1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#4A4949"
        android:layout_marginStart="15dp"
        android:layout_marginTop="18dp"
        android:text="@string/mentor_1"
        android:textSize="12sp"
        android:textStyle="bold"
        app:layout_constraintTop_toBottomOf="@+id/Recent"
        app:layout_constraintStart_toStartOf="parent"
        />
    <TextView
        android:id="@+id/m1cross"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="30dp"
        android:text="@string/cross"
        android:textSize="20sp"
        android:textStyle="bold"
        android:textColor="@color/darkgreen"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@id/M1"
        app:layout_constraintBottom_toBottomOf="@id/M1"
        />

    <TextView
        android:id="@+id/M2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#4A4949"
        android:layout_marginStart="15dp"
        android:layout_marginTop="18dp"
        android:text="@string/mentor_2"
        android:textSize="12sp"
        android:textStyle="bold"
        app:layout_constraintTop_toBottomOf="@+id/M1"
        app:layout_constraintStart_toStartOf="parent"
        />
    <TextView
        android:id="@+id/m2cross"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="30dp"
        android:text="@string/cross"
        android:textSize="20sp"
        android:textStyle="bold"
        android:textColor="@color/darkgreen"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@id/M2"
        app:layout_constraintBottom_toBottomOf="@id/M2"
        />
    <TextView
        android:id="@+id/M3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#4A4949"
        android:layout_marginStart="15dp"
        android:layout_marginTop="18dp"
        android:text="@string/mentor_3"
        android:textSize="12sp"
        android:textStyle="bold"
        app:layout_constraintTop_toBottomOf="@+id/M2"
        app:layout_constraintStart_toStartOf="parent"
        />
    <TextView
        android:id="@+id/m3cross"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="30dp"
        android:text="@string/cross"
        android:textSize="20sp"
        android:textStyle="bold"
        android:textColor="@color/darkgreen"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@id/M3"
        app:layout_constraintBottom_toBottomOf="@id/M3"
        />

    <TextView
        android:id="@+id/lowerbackground"
        android:layout_width="match_parent"
        android:layout_height="500dp"
        android:textColor="#4A4949"
        android:layout_marginTop="25dp"
        android:background="#B5EBE6"
        app:layout_constraintTop_toBottomOf="@+id/M3"
        app:layout_constraintStart_toStartOf="parent"
        />
    <TextView
        android:id="@+id/BrowseCategories"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/darkgreen"
        android:layout_marginStart="15dp"
        android:layout_marginTop="69dp"
        android:text="@string/browse_categories"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintTop_toBottomOf="@+id/M2"
        app:layout_constraintStart_toStartOf="parent"
        />
    <TextView
        android:id="@+id/Lowerbackground"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:background="#157177"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        />
    <ImageView
        android:id="@+id/circlebetweenlbandlr"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginBottom="-20dp"
        android:src="@drawable/darkgreencirclechat"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toTopOf="@id/Lowerbackground"
        android:contentDescription="@string/todo"/>

    <ImageView
        android:id="@+id/plus"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="-15dp"
        android:src="@drawable/aplus"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toTopOf="@id/Lowerbackground"
        android:contentDescription="@string/todo"/>
    <ImageView
        android:id="@+id/home"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="-9dp"
        app:layout_constraintStart_toStartOf="@id/Lowerbackground"
        app:layout_constraintBottom_toBottomOf="@id/Lowerbackground"
        app:layout_constraintTop_toTopOf="@id/Lowerbackground"
        app:srcCompat="@drawable/home"
        android:contentDescription="@string/todo"/>
    <TextView
        android:id="@+id/Hometyped"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/home"
        android:textColor="#4DA7A4"
        android:textSize="9sp"
        app:layout_constraintTop_toBottomOf="@id/home"
        app:layout_constraintStart_toStartOf="@+id/home"
        app:layout_constraintEnd_toEndOf="@+id/home"
        tools:ignore="SmallSp" />
    <ImageView
        android:id="@+id/search"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:src="@drawable/serachicon"
        android:layout_marginTop="-9dp"
        android:layout_marginStart="50dp"
        app:layout_constraintTop_toTopOf="@id/Lowerbackground"
        app:layout_constraintBottom_toBottomOf="@id/Lowerbackground"
        app:layout_constraintStart_toStartOf="@+id/home"
        app:layout_constraintEnd_toEndOf="@id/circlebetweenlbandlr"
        android:contentDescription="@string/todo" />
    <TextView
        android:id="@+id/searchtyped"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:text="@string/search"
        android:textColor="@color/white"
        android:textSize="9sp"
        app:layout_constraintTop_toBottomOf="@id/search"
        app:layout_constraintStart_toStartOf="@+id/search"
        app:layout_constraintEnd_toEndOf="@+id/search"
        tools:ignore="SmallSp" />

    <ImageView
        android:id="@+id/profile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="-9dp"
        android:layout_marginEnd="20dp"
        android:src="@drawable/profile"
        app:layout_constraintTop_toTopOf="@id/Lowerbackground"
        app:layout_constraintBottom_toBottomOf="@id/Lowerbackground"
        app:layout_constraintEnd_toEndOf="@id/Lowerbackground"
        android:contentDescription="@string/todo"/>

    <TextView
        android:id="@+id/profiletyped"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="-10dp"
        android:layout_marginBottom="2dp"
        android:text="@string/profile"
        android:textColor="#4C9C9A"
        android:textSize="9sp"
        app:layout_constraintTop_toBottomOf="@id/profile"
        app:layout_constraintStart_toStartOf="@+id/profile"
        app:layout_constraintEnd_toEndOf="@+id/profile"
        tools:ignore="SmallSp" />

    <ImageView
        android:id="@+id/message"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="-9dp"
        android:layout_marginEnd="40dp"
        app:srcCompat="@drawable/aaaaaa"
        app:layout_constraintTop_toTopOf="@id/Lowerbackground"
        app:layout_constraintBottom_toBottomOf="@id/Lowerbackground"
        app:layout_constraintStart_toStartOf="@id/circlebetweenlbandlr"
        app:layout_constraintEnd_toEndOf="@id/profile"
        android:contentDescription="@string/todo"/>

    <TextView
        android:id="@+id/messagetyped"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="-6dp"
        android:text="@string/chat"
        android:textColor="#7FB3B2"
        android:textSize="9sp"
        app:layout_constraintTop_toBottomOf="@id/message"
        app:layout_constraintStart_toStartOf="@+id/message"
        app:layout_constraintEnd_toEndOf="@+id/message"
        tools:ignore="SmallSp" />


</androidx.constraintlayout.widget.ConstraintLayout>