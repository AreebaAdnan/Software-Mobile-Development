<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/lightgreen"
    tools:context=".Page7">

    <ImageView
        android:id="@+id/TopImage"
        android:layout_width="250dp"
        android:layout_height="242dp"
        app:srcCompat="@drawable/e2d3b048a6d38fa3e27cc9fc7745fa3a"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:contentDescription="@string/todo" />

    <TextView
        android:id="@+id/Hello"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="100dp"
        android:layout_marginStart="25dp"
        android:textColor="@color/black"
        android:text="@string/hello"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/TopImage"
        app:layout_constraintStart_toStartOf="@+id/TopImage"
        />
    <TextView
        android:id="@+id/Ali"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:textColor="@color/darkgreen"
        android:text="@string/ali"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/Hello"
        app:layout_constraintStart_toEndOf="@+id/Hello"
        />
    <ImageView
        android:id="@+id/imageView"
        android:layout_width="40sp"
        android:layout_height="40sp"
        android:layout_marginEnd="30dp"
        app:srcCompat="@drawable/belliconpage7"
        app:layout_constraintTop_toTopOf="@+id/Hello"
        app:layout_constraintEnd_toEndOf="parent"
        android:contentDescription="@string/todo"
        />
    <EditText
        android:id="@+id/linebelowhello"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/ready_for_mentorship_let_s_begin_the_journey"
        android:gravity="start|top"
        android:inputType="textMultiLine"
        android:textColor="@color/darkgreen"
        android:textSize="20sp"
        app:layout_constraintTop_toBottomOf="@+id/Hello"
        app:layout_constraintStart_toStartOf="@+id/Hello"
        android:autofillHints=""
        tools:ignore="LabelFor" />

    <ImageView
        android:id="@+id/b1"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginTop="25dp"
        android:src="@drawable/darkestgreenrectangle"
        app:layout_constraintStart_toStartOf="@+id/Hello"
        app:layout_constraintTop_toBottomOf="@+id/linebelowhello"
        android:contentDescription="@string/todo" />
    <TextView
        android:id="@+id/b1text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/white"
        android:text="@string/all"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintTop_toTopOf="@+id/b1"
        app:layout_constraintStart_toStartOf="@+id/b1"
        app:layout_constraintBottom_toBottomOf="@+id/b1"
        app:layout_constraintEnd_toEndOf="@+id/b1"
        />
    <TextView
        android:id="@+id/mid"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/lightgreen"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        />

    <ImageView
        android:id="@+id/b2"
        android:layout_width="80dp"
        android:layout_height="40dp"
        app:layout_constraintTop_toTopOf="@+id/b1"
        android:src="@drawable/lightgreenrectanglechat"
        app:layout_constraintStart_toStartOf="@+id/b1"
        app:layout_constraintEnd_toEndOf="@+id/mid"
        app:layout_constraintTop_toBottomOf="@+id/linebelowhello"
        android:contentDescription="@string/todo" />
    <TextView
        android:id="@+id/b2text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/darkgreen"
        android:text="@string/education"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintTop_toTopOf="@+id/b2"
        app:layout_constraintStart_toStartOf="@+id/b2"
        app:layout_constraintBottom_toBottomOf="@+id/b2"
        app:layout_constraintEnd_toEndOf="@+id/b2"
        />
    <ImageView
        android:id="@+id/b3"
        android:layout_width="130dp"
        android:layout_height="40dp"
        android:layout_marginStart="-20dp"
        android:src="@drawable/lightgreenrectanglechat"
        app:layout_constraintTop_toTopOf="@+id/b1"
        app:layout_constraintStart_toStartOf="@+id/mid"
        app:layout_constraintTop_toBottomOf="@+id/linebelowhello"
        android:contentDescription="@string/todo" />
    <TextView
        android:id="@+id/b3text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/darkgreen"
        android:text="@string/enterpreneurship"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintTop_toTopOf="@+id/b3"
        app:layout_constraintStart_toStartOf="@+id/b3"
        app:layout_constraintBottom_toBottomOf="@+id/b3"
        app:layout_constraintEnd_toEndOf="@+id/b3"
        />
    <ImageView
        android:id="@+id/b4"
        android:layout_width="130dp"
        android:layout_height="40dp"
        android:layout_marginStart="50dp"
        android:src="@drawable/lightgreenrectanglechat"
        app:layout_constraintStart_toEndOf="@+id/b3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/b1"
        android:contentDescription="@string/todo"/>
    <TextView
        android:id="@+id/b4text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/darkgreen"
        android:text="@string/personal_growth"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintTop_toTopOf="@+id/b4"
        app:layout_constraintStart_toStartOf="@+id/b4"
        app:layout_constraintBottom_toBottomOf="@+id/b4"
        app:layout_constraintEnd_toEndOf="@+id/b4"
        />

    <TextView
        android:id="@+id/TopMemberforyou"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/darkgreen"
        android:layout_marginStart="5dp"
        android:layout_marginTop="15dp"
        android:text="@string/top_member_for_you"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintTop_toBottomOf="@+id/b1"
        app:layout_constraintStart_toStartOf="@+id/b1"
        />


    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/rv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="@id/TopMemberforyou"
        app:layout_constraintTop_toBottomOf="@+id/TopMemberforyou"
        android:orientation="horizontal"
        app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
        android:clipToPadding="false"
        tools:ignore="MissingConstraints"
        />

    <TextView
        android:id="@+id/Education"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/darkgreen"
        android:layout_marginStart="5dp"
        android:layout_marginTop="15dp"
        android:text="@string/education_mentor"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintTop_toBottomOf="@+id/rv"
        app:layout_constraintStart_toStartOf="@+id/rv"
        />
    <ImageView
        android:id="@+id/bb3"
        android:layout_width="200dp"
        android:layout_height="190dp"
        android:layout_marginTop="20dp"
        android:src="@drawable/greyrectangle"
        app:layout_constraintStart_toStartOf="@+id/b1"
        app:layout_constraintTop_toBottomOf="@+id/Education"
        android:contentDescription="@string/todo"
        />
    <ImageView
        android:id="@+id/bb3in"
        android:layout_width="200dp"
        android:layout_height="85dp"
        android:paddingBottom="10dp"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        android:src="@drawable/whiterectanglr"
        app:layout_constraintStart_toStartOf="@+id/b1"
        app:layout_constraintBottom_toBottomOf="@+id/bb3"
        android:contentDescription="@string/todo"/>
    <TextView
        android:id="@+id/bb3text1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/darkgreen"
        android:layout_marginTop="9dp"
        android:layout_marginStart="15dp"
        android:text="@string/micheal_drake"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintTop_toTopOf="@+id/bb3in"
        app:layout_constraintStart_toStartOf="@+id/bb3in"
        />
    <TextView
        android:id="@+id/bb3text12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#FFE500"
        android:layout_marginEnd="17dp"
        android:text="@string/_900_session"
        android:textSize="10sp"
        app:layout_constraintTop_toTopOf="@+id/bb3text1"
        app:layout_constraintBottom_toBottomOf="@+id/bb3text1"
        app:layout_constraintEnd_toEndOf="@+id/bb3in"
        tools:ignore="SmallSp" />

    <TextView
        android:id="@+id/bb3text2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/darkgreen"
        android:layout_marginTop="2dp"
        android:layout_marginStart="15dp"
        android:text="@string/nlp_expert"
        android:textSize="10sp"
        app:layout_constraintTop_toBottomOf="@+id/bb3text1"
        app:layout_constraintStart_toStartOf="@+id/bb3in"
        tools:ignore="SmallSp" />
    <TextView
        android:id="@+id/bb3text3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#00AA56"
        android:layout_marginTop="2dp"
        android:layout_marginStart="15dp"
        android:text="@string/available__"
        android:textSize="12sp"
        app:layout_constraintTop_toBottomOf="@+id/bb3text2"
        app:layout_constraintStart_toStartOf="@+id/bb3in"
        />
    <TextView
        android:id="@+id/bb3text4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/darkgreen"
        android:layout_marginTop="9dp"
        android:layout_marginEnd="18dp"
        android:text="@string/heart"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintTop_toTopOf="@+id/bb3text2"
        app:layout_constraintEnd_toEndOf="@+id/bb3in"
        />

    <ImageView
        android:id="@+id/bb4"
        android:layout_width="200dp"
        android:layout_height="190dp"
        android:layout_marginStart="50dp"
        android:src="@drawable/greyrectangle"
        app:layout_constraintStart_toStartOf="@+id/mid"
        app:layout_constraintTop_toTopOf="@+id/bb3"
        android:contentDescription="@string/todo"/>
    <ImageView
        android:id="@+id/bb4in"
        android:layout_width="200dp"
        android:layout_height="85dp"
        android:paddingBottom="10dp"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        android:src="@drawable/whiterectanglr"
        app:layout_constraintStart_toStartOf="@+id/bb4"
        app:layout_constraintBottom_toBottomOf="@+id/bb4"
        android:contentDescription="@string/todo"/>
    <TextView
        android:id="@+id/bb4text1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/darkgreen"
        android:layout_marginTop="9dp"
        android:layout_marginStart="15dp"
        android:text="@string/alex"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintTop_toTopOf="@+id/bb4in"
        app:layout_constraintStart_toStartOf="@+id/bb4in"
        />
    <TextView
        android:id="@+id/bb4text12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#FFE500"
        android:layout_marginEnd="10dp"
        android:text="@string/_200_session"
        android:textSize="10sp"
        app:layout_constraintTop_toTopOf="@+id/bb4text1"
        app:layout_constraintBottom_toBottomOf="@+id/bb4text1"
        app:layout_constraintEnd_toEndOf="@+id/bb4in"
        tools:ignore="SmallSp" />

    <TextView
        android:id="@+id/bb4text2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/black"
        android:layout_marginTop="2dp"
        android:layout_marginStart="15dp"
        android:text="@string/android_developer"
        android:textSize="10sp"
        app:layout_constraintTop_toBottomOf="@+id/bb4text1"
        app:layout_constraintStart_toStartOf="@+id/bb4in"
        tools:ignore="SmallSp" />
    <TextView
        android:id="@+id/bb4text3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#888888"
        android:layout_marginTop="2dp"
        android:layout_marginStart="15dp"
        android:text="@string/not_available_"
        android:textSize="12sp"
        app:layout_constraintTop_toBottomOf="@+id/bb4text2"
        app:layout_constraintStart_toStartOf="@+id/bb4in"
        />
    <TextView
        android:id="@+id/bb4text4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/darkgreen"
        android:layout_marginTop="9dp"
        android:layout_marginEnd="21dp"
        android:text="@string/heartw"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintTop_toTopOf="@+id/bb4text2"
        app:layout_constraintEnd_toEndOf="@+id/bb4in"
        />
    <TextView
        android:id="@+id/Lowerbackground"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:background="#157177"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        />
    <ImageView
        android:id="@+id/circlebetweenlbandlr"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginBottom="-20dp"
        android:src="@drawable/darkgreencirclechat"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toTopOf="@id/Lowerbackground"
        android:contentDescription="@string/todo"/>

    <ImageView
        android:id="@+id/plus"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="-15dp"
        android:src="@drawable/aplus"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toTopOf="@id/Lowerbackground"
        android:contentDescription="@string/todo"/>
    <ImageView
        android:id="@+id/home"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="-9dp"
        app:layout_constraintStart_toStartOf="@id/Lowerbackground"
        app:layout_constraintBottom_toBottomOf="@id/Lowerbackground"
        app:layout_constraintTop_toTopOf="@id/Lowerbackground"
        app:srcCompat="@drawable/home11"
        android:contentDescription="@string/todo"/>
    <TextView
        android:id="@+id/Hometyped"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/home"
        android:textColor="#FFFFFF"
        android:textSize="9sp"
        app:layout_constraintTop_toBottomOf="@id/home"
        app:layout_constraintStart_toStartOf="@+id/home"
        app:layout_constraintEnd_toEndOf="@+id/home"
        tools:ignore="SmallSp" />
    <ImageView
        android:id="@+id/search"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/search"
        android:layout_marginTop="-9dp"
        android:layout_marginStart="50dp"
        app:layout_constraintTop_toTopOf="@id/Lowerbackground"
        app:layout_constraintBottom_toBottomOf="@id/Lowerbackground"
        app:layout_constraintStart_toStartOf="@+id/home"
        app:layout_constraintEnd_toEndOf="@id/circlebetweenlbandlr"
        android:contentDescription="@string/todo" />
    <TextView
        android:id="@+id/searchtyped"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:text="@string/search"
        android:textColor="#4C9C9A"
        android:textSize="9sp"
        app:layout_constraintTop_toBottomOf="@id/search"
        app:layout_constraintStart_toStartOf="@+id/search"
        app:layout_constraintEnd_toEndOf="@+id/search"
        tools:ignore="SmallSp" />

    <ImageView
        android:id="@+id/profile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="-9dp"
        android:layout_marginEnd="20dp"
        android:src="@drawable/profile"
        app:layout_constraintTop_toTopOf="@id/Lowerbackground"
        app:layout_constraintBottom_toBottomOf="@id/Lowerbackground"
        app:layout_constraintEnd_toEndOf="@id/Lowerbackground"
        android:contentDescription="@string/todo"/>

    <TextView
        android:id="@+id/profiletyped"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="-10dp"
        android:layout_marginBottom="2dp"
        android:text="@string/profile"
        android:textColor="#4C9C9A"
        android:textSize="9sp"
        app:layout_constraintTop_toBottomOf="@id/profile"
        app:layout_constraintStart_toStartOf="@+id/profile"
        app:layout_constraintEnd_toEndOf="@+id/profile"
        tools:ignore="SmallSp" />

    <ImageView
        android:id="@+id/message"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="-9dp"
        android:layout_marginEnd="40dp"
        app:srcCompat="@drawable/aaaaaa"
        app:layout_constraintTop_toTopOf="@id/Lowerbackground"
        app:layout_constraintBottom_toBottomOf="@id/Lowerbackground"
        app:layout_constraintStart_toStartOf="@id/circlebetweenlbandlr"
        app:layout_constraintEnd_toEndOf="@id/profile"
        android:contentDescription="@string/todo"/>

    <TextView
        android:id="@+id/messagetyped"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="-6dp"
        android:text="@string/chat"
        android:textColor="#7FB3B2"
        android:textSize="9sp"
        app:layout_constraintTop_toBottomOf="@id/message"
        app:layout_constraintStart_toStartOf="@+id/message"
        app:layout_constraintEnd_toEndOf="@+id/message"
        tools:ignore="SmallSp" />



</androidx.constraintlayout.widget.ConstraintLayout>